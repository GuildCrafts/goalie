{
  "presets": [
    "es2015",
    "react",
    "airbnb",
    "stage-2"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["transform-regenerator", {"async": false}],
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
    }]
  ]
}
